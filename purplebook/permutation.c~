/*
* 三个三数等比排列，数字不能一样
*1:2:3 感觉暴力循环就完了
*
*/

#include <stdio.h>
int main()
{
	int i ;
	for(i=123; 3*i<988; i++)
	{
		if((i%10==0)||(2*i%10 == 0))
			;
		
		if(\
			(i/100%10)!=(i/10%10)&&(i/10%10)!=(i%10)&&\
			(2*i/100%10)!=(2*i/10%10)&&(2*i/10%10)!=(2*i%10)&&\
			(3*i/100%10)!=(3*i/10%10)&&(3*i/10%10)!=(3*i%10)&&\
			(i/100%10)!=(2*i/10%10)&&(2*i/10%10)!=(3*i%10)&&\
			(i/10%10)!=(2*i%10)&&(2*i%10)!=(3*i/100%10)&&\
			(2*i/100%10)!=(3*i/10%10)&&(3*i/10%10)!=(i%10)
  		  )
		  printf("%d %d %d\n",i,2*i,3*i);
	}
}


